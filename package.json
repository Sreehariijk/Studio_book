{
  "name": "studio_book",
  "version": "0.1.0",
  "private": true,
  "dependencies": {

    "font-awesome": "^4.7.0",
    "fullcalendar": "^3.9.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "classnames": "^2.2.6",
    "primeicons": "1.0.0-beta.9",
    "primereact": "2.0.0-alpha.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "axios": "^0.18.0", 
    "prime-react": "^1.0.0",
